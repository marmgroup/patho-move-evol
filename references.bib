}

@article{shizuka2016,
  title = {Measuring the Robustness of Network Community Structure Using Assortativity},
  author = {Shizuka, Daizaburo and Farine, Damien R.},
  year = {2016},
  month = feb,
  journal = {Animal Behaviour},
